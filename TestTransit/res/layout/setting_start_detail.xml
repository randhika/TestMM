            <include layout="@layout/divider_horizontal_line" />
            <LinearLayout android:orientation="vertical" android:id="@id/sound_vibration" android:background="@drawable/list_blue_background_selector" android:paddingLeft="@dimen/padding_normal" android:paddingTop="@dimen/padding_list_v" android:paddingRight="@dimen/padding_normal" android:paddingBottom="@dimen/padding_list_v" android:clickable="true" android:layout_width="fill_parent" android:layout_height="wrap_content" android:onClick="showVibDialog">
